<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Core.V1.xml">
    <edmx:Include Namespace="Org.OData.Core.V1" Alias="OData"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Measures.V1.xml">
    <edmx:Include Namespace="Org.OData.Measures.V1" Alias="Measures"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishExtensions_v1.xml">
    <edmx:Include Namespace="Validation.v1_0_0" Alias="Validation"/>
    <edmx:Include Namespace="RedfishExtensions.v1_0_0" Alias="Redfish"/>
  </edmx:Reference>
  <edmx:DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="Resource">
      <EntityType Name="Resource" Abstract="true"/>
      <EntityType Name="ResourceCollection" Abstract="true"/>
    </Schema>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ServiceRoot">
      <EntityType Name="ServiceRoot" BaseType="Resource.Resource" Abstract="true"/>
    </Schema>

    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ServiceRoot.v1_0_0">
      <EntityContainer Name="ServiceContainer">
        <Singleton Name="Service" Type="ServiceRoot.ServiceRoot"/>
      </EntityContainer>

      <EntityType Name="ServiceRoot" BaseType="ServiceRoot.ServiceRoot">
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record>
            <PropertyValue Property="Updatable" Bool="true"/>
          </Record>
        </Annotation>

        <Property Name="RedfishVersion" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="This is version of Redfish for testing purpose."/>
          <Annotation Term="OData.LongDescription" String="This is long description of Redfish version."/>
          <Annotation Term="Validation.Pattern" String="^\d+\.\d+\.\d+$"/>
        </Property>

        <Property Name="OptionalNullable" Type="Edm.String" Nullable="true">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </Property>

        <Property Name="ReadOnlyComplex" Type="ServiceRoot.v1_0_0.ReadOnlyComplexType" Nullable="true"/>

        <Property Name="Updatable" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/ReadWrite"/>
        </Property>

        <Property Name="UpdatableGuid" Type="Edm.Guid" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/ReadWrite"/>
        </Property>

        <Property Name="WriteOnly" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Write"/>
        </Property>

        <NavigationProperty Name="TestRequiredService" Nullable="false" Type="ServiceRoot.v1_0_0.TestRequiredService">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </NavigationProperty>

        <NavigationProperty Name="TestRequiredNullableService" Nullable="false" Type="ServiceRoot.v1_0_0.TestRequiredNullableService">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </NavigationProperty>

        <NavigationProperty Name="TestRequiredCollectionService" Nullable="false" Type="ServiceRoot.v1_0_0.TestRequiredCollectionService">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </NavigationProperty>

        <NavigationProperty Name="TestCollection" Nullable="false" Type="ServiceRoot.v1_0_0.TestCollection"/>

        <NavigationProperty Name="TestActionsService" Nullable="false" Type="ServiceRoot.v1_0_0.TestActionsService"/>

        <NavigationProperty Name="TestSettingsService" Nullable="false" Type="ServiceRoot.v1_0_0.TestSettingsService"/>

        <!-- Excerpt tests: links to excerpt target and ref entities -->
        <NavigationProperty Name="ExcerptEntity" Nullable="false" Type="ServiceRoot.v1_0_0.ExcerptEntity">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </NavigationProperty>

        <NavigationProperty Name="ExcerptRefEntity" Nullable="false" Type="ServiceRoot.v1_0_0.ExcerptRefEntity">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </NavigationProperty>
      </EntityType>

      <EntityType Name="TestRequiredService">
        <Property Name="Required" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="Redfish.Required"/>
        </Property>
      </EntityType>

      <EntityType Name="TestRequiredNullableService">
        <Property Name="RequiredNullable" Type="Edm.String" Nullable="true">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="Redfish.Required"/>
        </Property>
      </EntityType>

      <EntityType Name="TestRequiredCollectionService">
        <Property Name="RequiredCollection" Type="Collection(Edm.String)" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="Redfish.Required"/>
        </Property>
      </EntityType>

      <!-- This field resembles Resource.Status type that is marked -
           as read-only as whole. If it shouldn't be used in any
           updates even if property is not marked as read-only -->
      <ComplexType Name="ReadOnlyComplexType">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        <Property Name="Required" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </Property>
        <!-- Read only struct may include property with complex type
             but it still must be treated as read-only -->
        <Property Name="Level2" Type="ServiceRoot.v1_0_0.ComplexTypeLevel2" Nullable="false"/>
      </ComplexType>

      <ComplexType Name="ComplexTypeLevel2">
        <Property Name="Required" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </Property>
      </ComplexType>

      <!-- Typical collection EntityType in Redfish -->
      <EntityType Name="TestCollection">
        <Annotation Term="Capabilities.InsertRestrictions">
          <Record><PropertyValue Property="Insertable" Bool="true"/></Record>
        </Annotation>
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record><PropertyValue Property="Updatable" Bool="false"/></Record>
        </Annotation>
        <Annotation Term="Capabilities.DeleteRestrictions">
          <Record><PropertyValue Property="Deletable" Bool="false"/></Record>
        </Annotation>
        <NavigationProperty Name="Members" Type="Collection(ServiceRoot.v1_0_0.TestCollectionMember)">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.AutoExpandReferences"/>
          <Annotation Term="Redfish.Required"/>
        </NavigationProperty>
      </EntityType>

      <EntityType Name="TestCollectionMember"/>

      <!-- Updatable entity with Redfish Settings support -->
      <EntityType Name="TestSettingsService">
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record><PropertyValue Property="Updatable" Bool="true"/></Record>
        </Annotation>
        <Property Name="SettingValue" Type="Edm.String">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/ReadWrite"/>
        </Property>
      </EntityType>

      <!-- Typical actions in Redfish -->
      <EntityType Name="TestActionsService">
        <Property Name="Actions" Type="ServiceRoot.v1_0_0.TestActionsServiceActions" Nullable="false"/>
      </EntityType>

      <ComplexType Name="TestActionsServiceActions"/>
      <Action Name="TestAction" IsBound="true">
        <Parameter Name="TestActionsService" Type="ServiceRoot.v1_0_0.TestActionsServiceActions"/>
        <Parameter Name="ActionType" Type="ServiceRoot.v1_0_0.ActionType" Nullable="false"/>
      </Action>
      <EnumType Name="ActionType">
        <Member Name="Option1"/>
        <Member Name="Option2"/>
      </EnumType>

      <!-- Excerpt target entity: defines properties and excerpt keys -->
      <EntityType Name="ExcerptEntity">
        <Property Name="Always" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <!-- Included in all excerpt copies -->
          <Annotation Term="Redfish.Excerpt"/>
        </Property>
        <Property Name="BasicProp" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <!-- Only in keyed 'Basic' excerpt -->
          <Annotation Term="Redfish.Excerpt" String="Basic"/>
        </Property>
        <Property Name="DetailsProp" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <!-- Only in keyed 'Details' excerpt -->
          <Annotation Term="Redfish.Excerpt" String="Details"/>
        </Property>
        <!-- Not part of any excerpt (control) -->
        <Property Name="Hidden" Type="Edm.String" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </Property>
      </EntityType>

      <!-- Ref entity: three inline excerpt views to ExcerptEntity -->
      <EntityType Name="ExcerptRefEntity">
        <NavigationProperty Name="ExcerptAll" Type="ServiceRoot.v1_0_0.ExcerptEntity" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <!-- Inline excerpt (AllKeys) -->
          <Annotation Term="Redfish.ExcerptCopy"/>
        </NavigationProperty>
        <NavigationProperty Name="ExcerptBasic" Type="ServiceRoot.v1_0_0.ExcerptEntity" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <!-- Inline excerpt, keyed 'Basic' -->
          <Annotation Term="Redfish.ExcerptCopy" String="Basic"/>
        </NavigationProperty>
        <NavigationProperty Name="ExcerptDetails" Type="ServiceRoot.v1_0_0.ExcerptEntity" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <!-- Inline excerpt, keyed 'Details' -->
          <Annotation Term="Redfish.ExcerptCopy" String="Details"/>
        </NavigationProperty>
      </EntityType>

    </Schema>

    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="Settings">
      <ComplexType Name="Settings">
        <NavigationProperty Name="SettingsObject" Type="Resource.Resource" Nullable="false">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
        </NavigationProperty>
      </ComplexType>
      <ComplexType Name="PreferredApplyTime"/>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
